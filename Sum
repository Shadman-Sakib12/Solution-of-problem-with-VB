Question no. 1
Sub Main()
        Dim a As Integer
        Dim b As Integer
        Dim SOMA As Integer
        Console.WriteLine("Enter the first number")

        a = Console.ReadLine

        Console.WriteLine("Enter the second number")

        b = Console.ReadLine

        SOMA = a + b

        Console.WriteLine("SOMA = " & SOMA)
End Sub

 Question no. 2
 Sub Main()
        Dim number As Integer
        Dim a As Double
        Dim b As Integer
        Dim salary As Integer
        Console.WriteLine("Enter the number")

        number = Console.ReadLine

        Console.WriteLine("Enter the hours worked")

        a = Console.ReadLine

        Console.WriteLine("Enter the ammount paid per hour")

        b = Console.ReadLine

        salary = a * b

        Console.WriteLine("NUMBER = " & number)
        Console.WriteLine("SALARY = " & salary)



 End Sub


Question no. 3
Sub main()
        Dim a As Integer
        Dim b As Integer
        Dim c As Integer
        Dim d As Integer
        Dim distance As Double

        Console.WriteLine("Enter x1")

        a = Console.ReadLine

        Console.WriteLine("Enter x2")

        b = Console.ReadLine

        Console.WriteLine("Enter y1")

        c = Console.ReadLine

        Console.WriteLine("Enter y2")

        d = Console.ReadLine

        distance = Math.Sqrt(((b - a) * (b - a)) + ((d - c) * (d - c)))

        Console.WriteLine("distance = " & distance)

    End Sub
    
    
    
    Question no. 5
    Sub main()
      Dim a As Integer
        Dim b As Integer
        Dim total As Double
        Console.WriteLine("enter the serial number between ( 1 - 5 ) ")
        a = Console.ReadLine

        Console.WriteLine("enter the quantity ")
        b = Console.ReadLine


        If a = 1 Then
            total = b * 4

        ElseIf a = 2 Then

            total = b * 4.5

        ElseIf a = 3 Then

            total = b * 5

        ElseIf a = 4 Then

            total = b * 2

        ElseIf a = 5 Then

            total = b * 1.5

        End If


        Console.WriteLine("total : $" & total)


    End Sub

